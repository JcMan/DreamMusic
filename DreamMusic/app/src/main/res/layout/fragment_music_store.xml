<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:Jc="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
    <dream.app.com.dreammusic.ui.view.ViewPagerIndicator
        android:id="@+id/indicator_music_store"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/d_viewpagerindicator_height"
        android:background="@color/color_white"
        android:orientation="horizontal"
        >
    </dream.app.com.dreammusic.ui.view.ViewPagerIndicator>
    <View
        android:background="@color/color_split_line"
        android:layout_width="fill_parent"
        android:layout_height="0.5dp"/>
    <dream.app.com.dreammusic.ui.view.JazzyViewPager
        android:id="@+id/viewpager_music_store"
        android:overScrollMode="never"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">
    </dream.app.com.dreammusic.ui.view.JazzyViewPager>

</LinearLayout>
