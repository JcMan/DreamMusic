<dream.app.com.dreammusic.ui.view.SlideMenu
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:jc="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/slidemenu"
    android:background="@drawable/ic_index_background"
    tools:context=".MainActivity">
    >
    <RelativeLayout
        android:layout_width="@dimen/d_slidemenu_width"
        android:layout_height="fill_parent">
        <include layout="@layout/slidemenu_content"/>
     </RelativeLayout>
    <LinearLayout
       android:layout_width="fill_parent"
       android:layout_height="fill_parent"
        android:orientation="vertical"
        >
        <include layout="@layout/top"/>

    </LinearLayout>

</dream.app.com.dreammusic.ui.view.SlideMenu>

